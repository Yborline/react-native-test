{
  "expo": {
    "name": "Borys",
    "slug": "borys",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "borys",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}